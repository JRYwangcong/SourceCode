# change following line for default project
PROJ ?=  R2_MST9U3_ZUI_128MB

ifeq ($(PROJ),R2_MST9U3_ZUI_128MB)
    ################################################################################
    ##  BOARD_TYPE_SEL: BD_MST203A_A01A_S or BD_MST203A_A01B_S or BD_MST203B_A01A_S or BD_MST203C_A01A_S
    ################################################################################
    BOARD_TYPE_SEL ?= BD_MST9UHL_A0
    CC_MTOPTS += -DMS_BOARD_TYPE_SEL=$(BOARD_TYPE_SEL)
    CC_MTOPTS += -DMS_SW_CUS_SEL=SW_CONFIG_TRUNK_MST9U
    CHIP_FAMILY ?=MST9U
    LOADER ?= ./project/loader/target_MST9U_R2.ld
    BUILD_TARGET ?= ORGINAL_ALL_SYSTEM
    OS_TYPE = nos_aeon
    MMAP ?= project/mmap/MMAP_MST9U3_128_AEON.h
    CC_MTOPTS += -DMEMORY_MAP=MMAP_128MB
    MEMORY_SIZE = MEMORY_128MB
    ##########################################
    ##  MEM_TYPE: DDR3_1866 or DDR3_2133
    ##########################################
    MEM_TYPE = DDR2_1333
    BIN_FORMAT ?= COMPRESS
    ZUI ?= 0
    ENABLE_MSTV_UART_DEBUG ?= 1
    CC_MTOPTS += -DZUI=$(ZUI) -DENABLE_MSTV_UART_DEBUG=$(ENABLE_MSTV_UART_DEBUG)
    ##CC_MTOPTS += -DZUI=$(ZUI)
    QUICK_WAKEUP ?= 0
    CC_MTOPTS += -DQUICK_WAKEUP=$(QUICK_WAKEUP)
    BUILD_APLIB ?= n
    BUILD_LIB ?= n
    MST9U_FPGA ?= n
endif

